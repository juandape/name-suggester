{
  "name": "namer-suggester",
  "version": "1.0.1",
  "description": "A tool to analyze and improve variable and function names in JavaScript/TypeScript projects",
  "main": "dist/cjs/index.js",
  "type": "module",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "bin": {
    "namer-suggester": "./dist/esm/bin/namer-suggester.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc --project tsconfig.json --module ESNext --outDir dist/esm --declaration --declarationDir dist/types --emitDeclarationOnly false",
    "build:cjs": "tsc --project tsconfig.json --module CommonJS --outDir dist/cjs --declaration false",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types"
  },
  "keywords": [
    "javascript",
    "typescript",
    "naming",
    "refactoring",
    "code-quality",
    "react",
    "nextjs",
    "monorepo"
  ],
  "author": "Juan David Pe√±a <juandape@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.22.0",
    "@babel/traverse": "^7.22.0",
    "inquirer": "^9.2.0",
    "node-fetch": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juandape/name-suggester.git"
  },
  "bugs": {
    "url": "https://github.com/juandape/name-suggester/issues"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "typescript": "^5.9.2"
  }
}
